<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spay.module.member.dao.MemberDAO">

	<typeAliases>
	  <typeAlias alias="Member" type="com.spay.module.member.model.Member"/>
	</typeAliases>
    
    <select id="selectMemberByName" parameterType="String" resultType="com.spay.module.member.model.Member">
        SELECT * 
        FROM users
        WHERE username = #{name}
    </select>
    
    <insert id="insertMember" parameterType="Member" useGeneratedKeys="true" keyProperty="sessionKey">
        INSERT INTO users
        VALUES ()
    </insert>
    
    
</mapper>

